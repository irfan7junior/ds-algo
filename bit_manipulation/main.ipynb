   "source": [
    "## Count set bits in an integer"
   ],
   "cell_type": "markdown",
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "output_type": "execute_result",
     "data": {
      "text/plain": [
       "3"
      ]
     },
     "metadata": {},
     "execution_count": 3
    }
   ],
   "source": [
    "def count_set_bit(value: int) -> int:\n",
    "    count = 0\n",
    "    while value != 0:\n",
    "        if value % 2 == 1:\n",
    "            count += 1\n",
    "        value //= 2\n",
    "    return count\n",
    "\n",
    "# Testing\n",
    "count_set_bit(7)"
   ]
  }
 ]
